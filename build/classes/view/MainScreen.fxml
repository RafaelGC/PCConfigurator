<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?scenebuilder-stylesheet ../css/CustomButtons.css?>

<VBox fx:id="mainLayout" prefHeight="500.0" prefWidth="700.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.MainScreenController">
  <children>
    <Label text="Configura tu PC">
      <font>
        <Font size="36.0" />
      </font>
    </Label>
    <GridPane hgap="5.0">
      <children>
        <Label text="PC preconfigurados" GridPane.columnIndex="0" GridPane.rowIndex="0">
          <font>
            <Font size="18.0" fx:id="x1" />
          </font>
        </Label>
        <Label font="$x1" text="Tus configuraciones" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <Label font="$x1" text="Nueva configuraciÃ³n" GridPane.columnIndex="2" GridPane.rowIndex="0" />
        <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" GridPane.columnIndex="0" GridPane.rowIndex="1">
          <children>
            <Button maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-1.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="-1.0" style="&#10;" styleClass="big-button" text="PC Gama alta" />
            <Button maxWidth="1.7976931348623157E308" minHeight="-Infinity" mnemonicParsing="false" prefHeight="60.0" styleClass="big-button" text="PC Gama media" />
            <Button maxWidth="1.7976931348623157E308" minHeight="-Infinity" mnemonicParsing="false" prefHeight="60.0" styleClass="big-button" text="PC Gama baja" />
          </children>
        </VBox>
        <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
          <children>
            <Button maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-1.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="-1.0" style="&#10;" styleClass="big-button" text="Configurador paso a paso" />
            <Button maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-1.0" mnemonicParsing="false" onAction="#goToConfigurator" prefHeight="60.0" prefWidth="-1.0" style="&#10;" styleClass="big-button" text="Configurador avanzado" />
          </children>
        </VBox>
      </children>
      <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      </columnConstraints>
      <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="-1.0" vgrow="ALWAYS" />
      </rowConstraints>
    </GridPane>
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
</VBox>
